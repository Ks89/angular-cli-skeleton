{
  "name": "angular-cli-skeleton-server",
  "version": "1.0.0",
  "author": "Stefano Cappa",
  "description": "Angular skeleton with angular-cli - server side in koajs + Javascript",
  "license": "MIT",
  "homepage": "https://github.com/Ks89",
  "private": true,
  "repository": "https://github.com/Ks89/angular-cli-skeleton",
  "main": "src/bin/www",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.9.0"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon src/bin/www",
    "start:debug": "cross-env DEBUG=* node src/bin/www",
    "start:prod": "pm2 start pm2.json --env production",
    "stop:prod": "pm2 stop src/bin/www",
    "jshint": "jshint ."
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.10.0",
    "dotenv": "^8.2.0",
    "http-shutdown": "^1.2.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^5.2.0",
    "koa-jwt": "^3.6.0",
    "koa-logger": "^3.2.1",
    "koa-response-time": "^2.1.0",
    "koa-router": "^8.0.8",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.19",
    "request": "^2.88.0",
    "request-promise": "^4.2.5"
  },
  "devDependencies": {
    "cpr": "^3.0.1",
    "cross-env": "^7.0.2",
    "jshint": "^2.10.3",
    "nodemon": "^2.0.2",
    "pm2": "^4.1.2",
    "rimraf": "^3.0.0"
  }
}
