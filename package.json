{
  "name": "angular-cli-skeleton",
  "version": "1.0.0",
  "author": "Stefano Cappa",
  "description": "Angular skeleton with angular-cli",
  "license": "MIT",
  "main": "src/main.ts",
  "homepage": "https://github.com/Ks89",
  "private": true,
  "repository": "https://github.com/Ks89/angular-cli-skeleton",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:dev",
    "serve": "npm run serve:dev",
    "serve:dev": "ng serve --aot --hmr --proxy-config proxy.conf.json",
    "serve:dev:url": "ng serve --aot --hmr --proxy-config proxy.conf.json --base-href /your-custom-base-href/ --deploy-url /your-custom-base-href/",
    "serve:prod": "ng serve --prod --aot --proxy-config proxy.conf.json",
    "serve:prod:url": "ng serve --prod --aot --proxy-config proxy.conf.json --base-href /your-custom-base-href/ --deploy-url /your-custom-base-href/",
    "build": "npm run build:dev",
    "build:dev": "ng build --aot",
    "build:dev:stats": "npm run build:dev -- --stats-json",
    "build:dev:url": "ng build --aot --base-href /your-custom-base-href/ --deploy-url /your-custom-base-href/",
    "build:prod": "ng build --prod --aot --stats-json",
    "build:prod:stats": "npm run build:prod -- --stats-json",
    "build:prod:url": "ng build --prod --aot --base-href /your-custom-base-href/ --deploy-url /your-custom-base-href/",
    "build:verbose": "npm run build:dev -- --verbose",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "serve:prerender": "cd dist/angular-cli-skeleton && http-server",
    "generate:prerender": "cd dist && node prerender",
    "build:client-and-server-bundles": "ng build --prod --aot && ng run angular-cli-skeleton:server:production",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "clean": "npm cache clean --force && npm run clean:dist && npm run clean:sonar && npm run clean:coverage && npm run clean:docs",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "clean:sonar": "rimraf reports .scannerwork",
    "clean:docs": "rimraf docs documentation",
    "pretest": "npm run clean:coverage",
    "test": "ng test --watch=false --code-coverage=true",
    "test:snyk": "snyk test && npm run test",
    "test:watch": "ng test --watch=true --code-coverage=true",
    "test:ci": "ng test --watch=false --no-progress --code-coverage=true",
    "test:ci:jenkins": "cross-env-shell CI=true JENKINS=true ng test --watch=false --no-progress --code-coverage=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ee": "npm run e2e -- --element-explorer=true",
    "e2e:ci": "ng e2e --no-progress --config=protractor-ci.conf.js",
    "webdriver:update": "webdriver-manager update --gecko false --standalone false",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "docs": "npm run docs:typedoc && npm run docs:compodoc",
    "docs:typedoc": "typedoc --out ./docs/typedoc --mode file --target ES6 --exclude **/*.spec.ts ./src/",
    "docs:typedoc:serve": "npm run docs:typedoc && lite-server --baseDir=\"docs/typedoc\"",
    "docs:compodoc": "compodoc src -p tsconfig.json -d docs/compodoc",
    "docs:compodoc:serve": "npm run docs:compodoc -- -s",
    "docs:compodoc:serve:watch": "npm run docs:compodoc -- -s -w",
    "analyze:bundle": "webpack-bundle-analyzer dist/browser/stats.json",
    "analyze:sonar": "npm run clean && npm test && sonar-scanner -Dsonar.projectKey=angular-cli-skeleton -Dsonar.sources=. -Dsonar.host.url=https://sonarcloud.io -Dsonar.organization=ks89-github",
    "analyze:sonar:ci": "npm run clean && npm run test:ci && sonar-scanner -Dsonar.projectKey=angular-cli-skeleton -Dsonar.sources=. -Dsonar.host.url=https://sonarcloud.io -Dsonar.organization=ks89-github",
    "coveralls": "coveralls < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info",
    "prettier": "prettier --list-different --write \"**/*.*\"",
    "prettier:nowrite": "prettier --list-different \"**/*.*\"",
    "postinstall": "ivy-ngcc"
  },
  "pre-commit": [
    "prettier"
  ],
  "dependencies": {
    "@angular/animations": "~8.0.0-beta.7",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "~8.0.0-beta.7",
    "@angular/compiler": "~8.0.0-beta.7",
    "@angular/core": "~8.0.0-beta.7",
    "@angular/forms": "~8.0.0-beta.7",
    "@angular/http": "^8.0.0-beta.7",
    "@angular/platform-browser": "~8.0.0-beta.7",
    "@angular/platform-browser-dynamic": "~8.0.0-beta.7",
    "@angular/platform-server": "~8.0.0-beta.7",
    "@angular/pwa": "^0.13.5",
    "@angular/router": "~8.0.0-beta.7",
    "@angular/service-worker": "~8.0.0-beta.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@ng-bootstrap/ng-bootstrap": "^4.1.0",
    "@ngrx/store": "^7.3.0",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "bootstrap": "^4.1.3",
    "compression": "^1.7.3",
    "core-js": "^2.5.7",
    "domino": "^2.1.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "lodash": "^4.17.5",
    "ng2-validators": "^2.2.0",
    "ngx-toastr": "^9.1.0",
    "popper.js": "^1.14.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.14.0-beta.5",
    "@angular/cli": "~8.0.0-beta.5",
    "@angular/compiler-cli": "~8.0.0-beta.7",
    "@angular/language-service": "~8.0.0-beta.7",
    "@angularclass/hmr": "^2.1.3",
    "@compodoc/compodoc": "^1.1.5",
    "@ngrx/store-devtools": "^7.3.0",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.3.1",
    "@types/node": "^11.11.0",
    "codelyzer": "~4.5.0",
    "coveralls": "^3.0.1",
    "cross-env": "^5.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-sonarqube-unit-reporter": "0.0.18",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.0",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "ts-loader": "^4.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.13.1",
    "typedoc": "^0.14.2",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.2.3"
  },
  "greenkeeper": {
    "ignore": [
      "typescript",
      "@types/node",
      "ts-node"
    ]
  }
}
