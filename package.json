{
  "name": "angular-cli-skeleton",
  "version": "1.0.0-beta.1",
  "author": "Stefano Cappa",
  "description": "Angular skeleton with angular-cli",
  "license": "MIT",
  "main": "src/main.ts",
  "homepage": "https://github.com/Ks89",
  "private": true,
  "repository": "https://github.com/Ks89/angular-cli-skeleton",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.2.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:dev",
    "serve": "npm run serve:dev",
    "serve:dev": "ng serve --dev --hmr --show-circular-dependencies --proxy-config proxy.conf.json",
    "serve:dev:url": "ng serve --dev --hmr --proxy-config proxy.conf.json --base-href /your-custom-base-href/ --deploy-url /your-custom-base-href/",
    "serve:prod": "ng serve --prod  --aot --build-optimizer=true --proxy-config proxy.conf.json",
    "serve:prod:url": "ng serve --prod  --aot --build-optimizer=true --proxy-config proxy.conf.json --base-href /your-custom-base-href/ --deploy-url /your-custom-base-href/",
    "build": "npm run build:dev",
    "build:dev": "ng build --dev",
    "build:dev:stats": "npm run build:dev -- --stats-json",
    "build:dev:url": "ng build --dev --base-href /your-custom-base-href/ --deploy-url /your-custom-base-href/",
    "build:prod": "ng build --prod --aot --build-optimizer=true --stats-json",
    "build:prod:stats": "npm run build:prod -- --stats-json",
    "build:prod:url": "ng build --prod --aot --build-optimizer=true --base-href /your-custom-base-href/ --deploy-url /your-custom-base-href/",
    "build:verbose": "npm run build:dev -- --verbose",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "clean": "npm cache clean --force && npm run clean:dist && npm run clean:sonar && npm run clean:coverage && npm run clean:docs",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "clean:sonar": "rimraf reports .scannerwork",
    "clean:docs": "rimraf docs documentation",
    "pretest": "npm run clean:coverage",
    "test": "ng test --watch=false --code-coverage=true",
    "test:snyk": "snyk test && npm run test",
    "test:watch": "ng test --watch=true --code-coverage=true",
    "test:ci": "ng test --watch=false --no-progress --code-coverage=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ee": "npm run e2e -- --element-explorer=true",
    "e2e:ci": "ng e2e --no-progress --config=protractor-ci.conf.js",
    "webdriver:update": "webdriver-manager update --gecko false --standalone false",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "docs": "npm run docs:typedoc && npm run docs:compodoc",
    "docs:typedoc": "typedoc --out ./docs/typedoc --mode file --target ES6 --exclude **/*.spec.ts ./src/",
    "docs:typedoc:serve": "npm run docs:typedoc && lite-server --baseDir=\"docs/typedoc\"",
    "docs:compodoc": "compodoc src -p tsconfig.json -d docs/compodoc",
    "docs:compodoc:serve": "npm run docs:compodoc -- -s",
    "docs:compodoc:serve:watch": "npm run docs:compodoc -- -s -w",
    "analyze:bundle": "webpack-bundle-analyzer dist/browser/stats.json",
    "analyze:sonar": "npm run clean && npm test && sonar-scanner -Dsonar.projectKey=angular-cli-skeleton -Dsonar.sources=. -Dsonar.host.url=https://sonarcloud.io -Dsonar.organization=ks89-github",
    "analyze:sonar:ci": "npm run clean && npm run test:ci && sonar-scanner -Dsonar.projectKey=angular-cli-skeleton -Dsonar.sources=. -Dsonar.host.url=https://sonarcloud.io -Dsonar.organization=ks89-github",
    "coveralls": "coveralls < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "dependencies": {
    "@angular/animations": "^5.1.2",
    "@angular/common": "^5.1.2",
    "@angular/compiler": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/forms": "^5.1.2",
    "@angular/http": "^5.1.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/platform-browser-dynamic": "^5.1.2",
    "@angular/platform-server": "^5.1.2",
    "@angular/router": "^5.1.2",
    "@angular/service-worker": "^5.1.2",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.8",
    "@ngrx/store": "^4.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "bootstrap": "^4.0.0-beta.2",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "ng2-validators": "^2.2.0",
    "popper.js": "^1.12.9",
    "rxjs": "^5.5.6",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "ts-loader": "^3.2.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.3",
    "@angular/compiler-cli": "^5.1.2",
    "@angular/language-service": "^5.1.2",
    "@angularclass/hmr": "^2.1.3",
    "@compodoc/compodoc": "^1.0.5",
    "@ngrx/store-devtools": "^4.1.1",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/jquery": "^3.2.17",
    "@types/node": "~8.5.2",
    "@types/socket.io": "^1.4.31",
    "@types/socket.io-client": "^1.4.32",
    "codelyzer": "^4.0.1",
    "coveralls": "^3.0.0",
    "http-server": "^0.10.0",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-sonarqube-unit-reporter": "0.0.14",
    "protractor": "^5.2.2",
    "rimraf": "^2.6.2",
    "ts-node": "~4.1.0",
    "tslint": "^5.8.0",
    "typedoc": "^0.9.0",
    "typescript": "2.6.2",
    "webdriver-manager": "^12.0.6",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "browserslist": [
    "last 2 versions",
    "not ie <= 10",
    "not ie_mob <= 10"
  ],
  "greenkeeper": {
    "ignore": [
      "typescript",
      "@types/node",
      "ts-node"
    ]
  }
}
