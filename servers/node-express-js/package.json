{
  "name": "angular-cli-skeleton-server",
  "version": "1.0.0",
  "author": "Stefano Cappa",
  "description": "Angular skeleton with angular-cli - server side in Javascript",
  "license": "MIT",
  "homepage": "https://github.com/Ks89",
  "private": true,
  "repository": "https://github.com/Ks89/angular-cli-skeleton",
  "main": "bin/www",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.9.0"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon bin/www",
    "start:debug": "cross-env DEBUG=* node bin/www",
    "start:prod": "pm2 start pm2.json --env production",
    "stop:prod": "pm2 stop bin/www",
    "jshint": "jshint ."
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "expect-ct": "^0.3.0",
    "express": "^4.16.3",
    "express-content-length-validator": "^1.0.0",
    "express-rate-limit": "^5.1.1",
    "express-session": "^1.15.6",
    "express-slow-down": "^1.3.1",
    "helmet": "^3.21.3",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "cpr": "^3.0.1",
    "cross-env": "^7.0.2",
    "jshint": "^2.11.0",
    "nodemon": "^2.0.2",
    "pm2": "^4.2.3",
    "rimraf": "^3.0.2"
  }
}
