{
  "name": "angular-cli-skeleton",
  "version": "1.0.0-beta.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:dev",
    "serve": "npm run serve:dev",
    "serve:dev": "ng serve --dev --hmr --show-circular-dependencies",
    "serve:dev:url": "ng serve --dev --hmr --base-href /your-custom-base-href/ --deploy-url /your-custom-base-href/",
    "serve:prod": "ng serve --prod",
    "serve:prod:url": "ng serve --prod --base-href /your-custom-base-href/ --deploy-url /your-custom-base-href/",
    "serve:prod:aot": "ng serve --prod --aot",
    "serve:prod:aot:url": "ng serve --prod --aot --base-href /your-custom-base-href/ --deploy-url /your-custom-base-href/",
    "build": "npm run build:dev",
    "build:dev": "ng build --dev",
    "build:dev:url": "ng build --dev --base-href /your-custom-base-href/ --deploy-url /your-custom-base-href/",
    "build:prod": "ng build --prod",
    "build:prod:url": "ng build --prod --base-href /your-custom-base-href/ --deploy-url /your-custom-base-href/",
    "build:prod:aot": "ng build --prod --aot",
    "build:prod:aot:url": "ng build --prod --aot --base-href /your-custom-base-href/ --deploy-url /your-custom-base-href/",
    "build:verbose": "npm run build:dev -- --verbose",
    "clean": "npm cache clean --force && npm run clean:dist && npm run clean:aot && npm run clean:coverage && npm run clean:docs",
    "clean:dist": "rimraf dist",
    "clean:aot": "rimraf compiled",
    "clean:coverage": "rimraf coverage",
    "clean:docs": "rimraf docs && rimraf documentation",
    "pretest": "npm run clean:coverage",
    "test": "ng test --watch=false --code-coverage=true",
    "test:watch": "ng test --watch=true --code-coverage=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ee": "npm run e2e -- --element-explorer=true",
    "webdriver:update": "webdriver-manager update --gecko false --standalone false",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "docs": "typedoc --out docs src",
    "docs:compodoc": "compodoc -p tsconfig.json",
    "docs:compodoc:serve": "compodoc -p tsconfig.json -s",
    "docs:compodoc:serve:watch": "compodoc -p tsconfig.json -s -w",
    "sonar-scanner": "sonar-scanner",
    "coveralls": "coveralls < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.1",
    "@angular/common": "^5.1.1",
    "@angular/compiler": "^5.1.1",
    "@angular/core": "^5.1.1",
    "@angular/forms": "^5.1.1",
    "@angular/http": "^5.1.1",
    "@angular/platform-browser": "^5.1.1",
    "@angular/platform-browser-dynamic": "^5.1.1",
    "@angular/router": "^5.1.1",
    "@angular/service-worker": "^5.1.1",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.7",
    "@ngrx/store": "^4.1.1",
    "bootstrap": "^4.0.0-beta.2",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "ng2-validators": "^2.2.0",
    "popper.js": "^1.12.9",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.0",
    "@angular/compiler-cli": "^5.1.1",
    "@angular/language-service": "^5.1.1",
    "@compodoc/compodoc": "^1.0.5",
    "@ngrx/store-devtools": "^4.1.1",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "coveralls": "^3.0.0",
    "http-server": "^0.10.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-diff-reporter": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-remap-coverage": "^0.1.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sonarqube-unit-reporter": "0.0.14",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typedoc": "^0.9.0",
    "typescript": "=2.5.3",
    "webdriver-manager": "^12.0.6"
  }
}
