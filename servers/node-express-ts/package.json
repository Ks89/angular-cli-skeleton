{
  "name": "angular-cli-skeleton-server",
  "version": "1.0.0",
  "author": "Stefano Cappa",
  "description": "Angular skeleton with angular-cli - server side in Typescript",
  "license": "MIT",
  "homepage": "https://github.com/Ks89",
  "private": true,
  "repository": "https://github.com/Ks89/angular-cli-skeleton",
  "main": "bin/www",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.2.0"
  },
  "scripts": {
    "start": "npm run build && nodemon dist/server.js",
    "build": "npm run build:ts && npm run tslint",
    "build:ts": "tsc",
    "serve": "node dist/server.js",
    "serve:debug": "nodemon --inspect dist/server.js",
    "watch:node": "nodemon dist/server.js",
    "watch:ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "dotenv": "^5.0.1",
    "expect-ct": "^0.1.0",
    "express": "^4.16.3",
    "express-content-length-validator": "^1.0.0",
    "express-rate-limit": "^2.11.0",
    "express-session": "^1.15.6",
    "helmet": "^3.12.0",
    "hpp": "^0.2.2",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/csurf": "^1.9.35",
    "@types/debug": "0.0.30",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@types/express-rate-limit": "^2.9.3",
    "@types/express-session": "^1.15.8",
    "@types/helmet": "0.0.37",
    "@types/hpp": "^0.2.1",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/lodash": "^4.14.108",
    "@types/node": "^10.0.10",
    "@types/nodemailer": "^4.3.4",
    "@types/passport": "^0.4.5",
    "@types/passport-jwt": "^3.0.1",
    "@types/winston": "^2.3.9",
    "cpr": "^3.0.1",
    "cross-env": "^5.1.5",
    "jshint": "^2.9.5",
    "nodemon": "^1.17.4",
    "pm2": "^2.10.3",
    "rimraf": "^2.6.2",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  }
}
